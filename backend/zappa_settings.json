{
  "production": {
    "app_function": "app.handler",
    "aws_region": "ap-northeast-1",
    "exclude": [
      "migrations",
      "seeddata",
      "boto3",
      "dateutil",
      "botocore",
      "s3transfer",
      "concurrent"
    ],
    "profile_name": "sf-wikitool",
    "project_name": "sf-wikitool-backend",
    "runtime": "python3.12",
    "apigateway_enabled": true,
    "cors": true,
    "s3_bucket": "sf-wikitool-backend"
  }
}
